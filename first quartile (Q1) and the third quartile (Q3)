# Given age data
ages <- c(13, 15, 16, 16, 19, 20, 20, 21, 22, 22, 25, 25, 25, 25, 30, 
          33, 33, 35, 35, 35, 35, 36, 40, 45, 46, 52, 70)

# Compute Q1 and Q3
Q1 <- quantile(ages, 0.25)  # First quartile (25th percentile)
Q3 <- quantile(ages, 0.75)  # Third quartile (75th percentile)

# Print results
print(paste("First Quartile (Q1):", Q1))
print(paste("Third Quartile (Q3):", Q3))
